{
  "name": "karelwintersky/steamboatengine",
  "description": "Steamboat Engine",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Karel Wintersky",
      "email": "karel.wintersky@gmail.com"
    }
  ],
  "config": {
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=7",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "karelwintersky/arris": "^1.52",
    "cmfcmf/openweathermap-php-api": "^3.2"
  },
  "require-dev": {
    "monolog/monolog": "^1.23",
    "foolz/sphinxql-query-builder": "^2.0",
    "smarty/smarty": "^3.1"
  },
  "autoload": {
    "files": [
      "functions/functions.php",
      "functions/helpers.php"
    ],
    "psr-4": {
      "SteamBoat\\"           : ["sources/", "interfaces/"]
    }
  }
}
